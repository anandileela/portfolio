#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anandiknuppel/portfolio/node_modules/.pnpm/astro@5.15.4_@types+node@24.10.0_jiti@2.6.1_lightningcss@1.29.3_rollup@4.53.2_sass@1.93.3_terser@5.39.0_typescript@5.9.3/node_modules/astro/node_modules:/Users/anandiknuppel/portfolio/node_modules/.pnpm/astro@5.15.4_@types+node@24.10.0_jiti@2.6.1_lightningcss@1.29.3_rollup@4.53.2_sass@1.93.3_terser@5.39.0_typescript@5.9.3/node_modules:/Users/anandiknuppel/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anandiknuppel/portfolio/node_modules/.pnpm/astro@5.15.4_@types+node@24.10.0_jiti@2.6.1_lightningcss@1.29.3_rollup@4.53.2_sass@1.93.3_terser@5.39.0_typescript@5.9.3/node_modules/astro/node_modules:/Users/anandiknuppel/portfolio/node_modules/.pnpm/astro@5.15.4_@types+node@24.10.0_jiti@2.6.1_lightningcss@1.29.3_rollup@4.53.2_sass@1.93.3_terser@5.39.0_typescript@5.9.3/node_modules:/Users/anandiknuppel/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../astro@5.15.4_@types+node@24.10.0_jiti@2.6.1_lightningcss@1.29.3_rollup@4.53.2_sass@1.93.3_terser@5.39.0_typescript@5.9.3/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../astro@5.15.4_@types+node@24.10.0_jiti@2.6.1_lightningcss@1.29.3_rollup@4.53.2_sass@1.93.3_terser@5.39.0_typescript@5.9.3/node_modules/astro/astro.js" "$@"
fi
